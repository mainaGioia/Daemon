<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FinetunerApp.Views.FABPage"
             xmlns:vm="clr-namespace:FinetunerApp.ViewModels;"
             xmlns:controls="clr-namespace:FinetunerApp.Controls"
             Title="FAB">
    <StackLayout Margin="32">
        <controls:FloatingActionButton x:Name="FAB" 
                                       HorizontalOptions="EndAndExpand" 
                                       WidthRequest="50"
                                       HeightRequest="50"
                                       VerticalOptions="EndAndExpand"
                                       Image="plus.png"
                                       ButtonColor="#03A9F4"
                                       Command="{Binding OnClickFab}"/>
    </StackLayout>

   
</ContentPage>